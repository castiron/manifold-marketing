{
    "name": "castiron/contentment-plugin",
    "description": "An October CMS plugin for managing pages and content and keeping it in the database. ",
    "homepage": "https://github.com/castiron/contentment-plugin",
    "keywords": ["october", "cms", "octobercms", "laravel", "pages", "plugin", "content", "contentment"],
    "type": "october-plugin",
    "version": "0.0.1",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:castiron/peaches.git"
        }
    ],
    "support": {
        "issues": "https://github.com/octobercms/october/issues",
        "forum": "http://octobercms.com/forum/",
        "docs": "http://octobercms.com/docs/",
        "irc": "irc://irc.freenode.net/october",
        "source": "https://github.com/octobercms/october"
    },
    "require": {
        "php": ">=5.4",
        "castiron/peaches": "dev-master",
        "owl/list-delete": "~1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Castiron\\": "plugins/castiron/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
